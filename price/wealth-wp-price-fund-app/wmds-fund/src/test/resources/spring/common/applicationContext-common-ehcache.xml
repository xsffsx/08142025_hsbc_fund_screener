<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd"
         updateCheck="false" monitoring="autodetect"
         dynamicConfig="true" name="ehcache_market">
    <cache name="advanceChartCache"  eternal="false"
		overflowToDisk="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		diskPersistent="false" memoryStoreEvictionPolicy="LRU"  maxBytesLocalHeap ="100M">
	</cache>
    <cache name="advanceChartCoreCache"  eternal="false"
		overflowToDisk="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		diskPersistent="false" memoryStoreEvictionPolicy="LRU" maxBytesLocalHeap ="100M">
	</cache>
</ehcache>