<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.hhhh.wealth.mds</groupId>
<!--	<groupId>com.hhhh.group.secwealth.mktdata.equity.index</groupId>-->
	<artifactId>wmds-equity-index-quotes</artifactId>

	<version>1.0.0-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>wmds-equity-index-quotes</name>
	<description>equity index quotes api and implemention</description>
	<parent>
		<groupId>com.hhhh.group.secwealth.mktdata.starter</groupId>
		<artifactId>parent-spring-boot-starter</artifactId>
		<version>1.0.0-HASE-RELEASE</version>
		<relativePath>../parent-spring-boot-starter/pom.xml</relativePath>
	</parent>

	<properties>
		<springframework.boot.version>1.5.17.RELEASE</springframework.boot.version>
		<jacoco-maven-plugin.version>0.8.2</jacoco-maven-plugin.version>
		<sonar.coverage.jacoco.xmlReportPaths>target/jacoco.xml</sonar.coverage.jacoco.xmlReportPaths>
		<sonar.exclusions>
			**/src/main/java/com/hhhh/group/secwealth/mktdata/api/equity/index/service/IndexQuotesCounterUSService.java,
			**/src/main/java/com/hhhh/group/secwealth/mktdata/api/equity/index/service/IndexQuotesLogUSService.java,
			**/src/main/java/com/hhhh/group/secwealth/mktdata/api/equity/index/service/QuoteAccessLogService.java,
			**/src/main/java/com/hhhh/group/secwealth/mktdata/api/equity/index/service/QuoteUserService.java,
			**/src/main/java/com/hhhh/group/secwealth/mktdata/api/equity/index/dao/**,
			**/src/main/java/com/hhhh/group/secwealth/mktdata/api/equity/index/request/**,
			**/src/main/java/com/hhhh/group/secwealth/mktdata/api/equity/index/response/**,
			**/src/main/java/com/hhhh/group/secwealth/mktdata/api/equity/index/controller/**,
			**/src/main/java/com/hhhh/group/secwealth/mktdata/api/equity/ServerLauncher.java
		</sonar.exclusions>
		<sonar.java.coveragePlugin>jacoco</sonar.java.coveragePlugin>
		<sonar.dynamicAnalysis>reuseReports</sonar.dynamicAnalysis>
		<sonar.junit.reportPaths>target/surefire-reports</sonar.junit.reportPaths>
		<sonar.jacoco.reportPaths>target/jacoco.exec</sonar.jacoco.reportPaths>
	</properties>

	<profiles>
		<profile>
			<id>releaseVersion</id>
			<properties>
				<project.version>1.0.0-RELEASE</project.version>
			</properties>
		</profile>
		<!-- HASE STMA PROD EQUITY -->
		<profile>
			<id>HASE_STMA_EQTY</id>
			<properties>
				<environment>skm_hase_stma_eqty</environment>
				<project.version>1.0.0-HASE_STMA_EQTY</project.version>
				<project.packaging>war</project.packaging>
			</properties>
		</profile>

		<!-- HASE STMA DEV EQUITY -->
		<profile>
			<id>DEV_HASE_STMA_EQTY</id>
			<properties>
				<environment>dev_hase_stma_eqty</environment>
				<project.version>1.0.0-DEV_HASE_STMA_EQTY-SNAPSHOT</project.version>
				<project.packaging>war</project.packaging>
			</properties>
		</profile>
		<!-- HASE STMA SIT EQUITY -->
		<profile>
			<id>SIT_HASE_STMA_EQTY</id>
			<properties>
				<environment>sit_hase_stma_eqty</environment>
				<project.version>1.0.0-SIT_HASE_STMA_EQTY-SNAPSHOT</project.version>
				<project.packaging>war</project.packaging>
			</properties>
		</profile>

		<!-- HASE STMA UAT EQUITY -->
		<profile>
			<id>UAT_HASE_STMA_EQTY</id>
			<properties>
				<environment>uat_hase_stma_eqty</environment>
				<project.version>1.0.0-UAT_HASE_STMA_EQTY-SNAPSHOT</project.version>
				<project.packaging>war</project.packaging>
			</properties>
		</profile>
	</profiles>

	<dependencies>
		<dependency>
			<groupId>com.hhhh.group.secwealth.mktdata.starter</groupId>
			<artifactId>cache-distribute-handler-spring-boot-starter</artifactId>
			<version>1.0.5-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.hhhh.wmd</groupId>
			<artifactId>common-logging-util</artifactId>
			<version>1.3.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-data-jpa</artifactId>
		</dependency>
		<dependency>
			<groupId>com.hhhh.group.secwealth.mktdata.api.equity.common</groupId>
			<artifactId>wmds-equity-common</artifactId>
			<version>1.0.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.hhhh.group.secwealth.mktdata.starter</groupId>
			<artifactId>service-dispatcher-spring-boot-starter</artifactId>
			<version>1.0.1-HASE-RELEASE</version>
		</dependency>
		<dependency>
			<groupId>com.sun.crypto.provider</groupId>
			<artifactId>SunJCE</artifactId>
			<version>1.3.1</version>
		</dependency>
		<dependency>
			<groupId>com.hhhh.wmd.itt</groupId>
			<artifactId>wealth-e2etrust-utils-spring-boot-starter</artifactId>
			<version>1.8.0</version>
		</dependency>

		<dependency>
			<groupId>javax.xml.bind</groupId>
			<artifactId>jaxb-api</artifactId>
			<version>2.3.1</version>
		</dependency>

		<dependency>
			<groupId>com.hhhh.group.secwealth.mktdata</groupId>
			<artifactId>wmds-entitlement</artifactId>
			<version>1.0.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>org.junit.jupiter</groupId>
			<artifactId>junit-jupiter-api</artifactId>
			<version>RELEASE</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-test</artifactId>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>com.h2database</groupId>
			<artifactId>h2</artifactId>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.22.1</version>
				<configuration>
					<forkCount>1</forkCount>
					<reuseForks>true</reuseForks>
					<testFailureIgnore>true</testFailureIgnore>
					<redirectTestOutputToFile>true</redirectTestOutputToFile>
					<systemPropertyVariables>
						<jacoco-agent.destfile>${project.build.directory}/jacoco.exec</jacoco-agent.destfile>
					</systemPropertyVariables>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.jacoco</groupId>
				<artifactId>jacoco-maven-plugin</artifactId>
				<version>${jacoco-maven-plugin.version}</version>
				<configuration>
					<excludes>
					</excludes>
				</configuration>
				<executions>
					<execution>
						<id>default-prepare-agent</id>
						<goals>
							<goal>prepare-agent</goal>
						</goals>
					</execution>
					<execution>
						<id>jacoco-initialize</id>
						<goals>
							<goal>prepare-agent</goal>
						</goals>
					</execution>
					<execution>
						<id>jacoco-site</id>
						<phase>package</phase>
						<goals>
							<goal>report</goal>
						</goals>
						<configuration>
							<dataFile>${project.build.directory}/jacoco.exec</dataFile>
						</configuration>
					</execution>
				</executions>
			</plugin>

		</plugins>
	</build>
	<distributionManagement>
		<repository>
			<id>nexus306</id>
			<url>https://nexus306.systems.uk.hhhh:8081/nexus/repository/maven-hhhh-internal-prod_n3p/</url>
		</repository>
		<snapshotRepository>
			<id>nexus306</id>
			<url>https://nexus306.systems.uk.hhhh:8081/nexus/repository/maven-hhhh-internal-snapshots_n3p/</url>
		</snapshotRepository>
	</distributionManagement>

</project>