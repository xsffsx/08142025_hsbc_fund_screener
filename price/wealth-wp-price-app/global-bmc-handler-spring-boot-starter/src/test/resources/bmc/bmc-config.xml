<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bmcEntitys>
<bmcEntitys>
	<!-- BMC file path -->
	<bmcFilePath></bmcFilePath>
	<!-- If generate BMC files -->
	<printFiles>false</printFiles>
	<!-- Get the bmcEntity by prefix + countryCode + groupMember -->
	<bmcEntity prefix="Default" countryCode="" groupMember="">
		<!-- In <exTimeDurationThrownPastSec> seconds, <exNumThrownPastSec> errors occurred, trigger <exCdeThrownPastSec> and <exMgsThrownPastSec> alert -->
		<exTimeDurationThrownPastSec></exTimeDurationThrownPastSec>
		<exNumThrownPastSec></exNumThrownPastSec>
		<exCdeThrownPastSec></exCdeThrownPastSec>
		<exMgsThrownPastSec></exMgsThrownPastSec>
		<!-- When the error is accumulated to <exNumExceedLimit>, trigger <exCdeExceedLimit> and <exMgsExceedLimit> -->
		<exNumExceedLimit></exNumExceedLimit>
		<exCdeExceedLimit></exCdeExceedLimit>
		<exMgsExceedLimit></exMgsExceedLimit>
		<exceptionCounter exceptionName="" key="Undefined">
			<timeDuration></timeDuration>
			<maxNum></maxNum>
			<bmcExCde></bmcExCde>
			<bmcExMsg></bmcExMsg>
		</exceptionCounter>
	</bmcEntity>
</bmcEntitys>
