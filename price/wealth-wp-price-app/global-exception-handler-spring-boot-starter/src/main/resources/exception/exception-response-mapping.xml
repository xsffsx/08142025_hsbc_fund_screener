<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping>
<mapping xmlns:cst="http://castor.exolab.org/">
	<class
		cst:name="com.hhhh.group.secwealth.mktdata.starter.global_exception_handler.response.entity.ExResponseEntitys">
		<map-to cst:xml="exResponseEntitys" />
		<field cst:name="exResponseEntityMap" collection="map">
			<bind-xml name="exResponseEntity">
				<class name="org.exolab.castor.mapping.MapItem">
					<field name="key" type="java.lang.String">
						<bind-xml name="key" node="attribute" />
					</field>
					<field name="value"
						type="com.hhhh.group.secwealth.mktdata.starter.global_exception_handler.response.entity.ExResponseEntity" />
				</class>
			</bind-xml>
		</field>
	</class>

	<class
		cst:name="com.hhhh.group.secwealth.mktdata.starter.global_exception_handler.response.entity.ExResponseEntity">
		<map-to cst:xml="entity" />
		<field cst:name="responseCode" cst:type="java.lang.String">
			<bind-xml name="responseCode" node="element" />
		</field>
		<field cst:name="reasonCode" cst:type="java.lang.String">
			<bind-xml name="reasonCode" node="element" />
		</field>
		<field cst:name="text" cst:type="java.lang.String">
			<bind-xml name="text" node="element" />
		</field>
		<field cst:name="status" cst:type="java.lang.String">
			<bind-xml name="status" node="element" />
		</field>
	</class>
</mapping>
    