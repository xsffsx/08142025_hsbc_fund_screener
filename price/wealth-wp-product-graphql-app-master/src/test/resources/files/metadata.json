    {
      "_id": "TB_PROD.PROD_LNCH_DT",
      "code": "29",
      "table": "TB_PROD",
      "fieldName": "PROD_LNCH_DT",
      "fieldScale": 0,
      "fieldPrecision": 0,
      "fieldType": 93,
      "fieldTypeName": "DATE",
      "fieldNullable": 1,
      "attrName": "prodLnchDt",
      "jsonPath": "prodLnchDt",
      "parent": "[ROOT]",
      "instrument": "ALL",
      "classification": "SMD",
      "mandatory": "M if not dummy product",
      "dataUsage": null,
      "logicalEntityName": "Product",
      "sampleData": null,
      "businessName": "Product Launch Date",
      "businessDefinition": "Denotes the date on which the product launched to the market.",
      "graphQLType": "Date",
      "validations": [
        {
          "given": "dmyProdSubtpRecInd!='Y' and prodTypeCde != 'SID' and prodId == null",
          "rules": {
            "required": true,
            "range": {
              "min": "1900-01-01",
              "max": "2100-12-31"
            }
          }
        }
      ]
    },
    {
      "_id": "TB_PROD.CCY_PROD_MKT_PRC_CDE",
      "table": "TB_PROD",
      "fieldName": "CCY_PROD_MKT_PRC_CDE",
      "fieldScale": 0,
      "fieldPrecision": 3,
      "fieldType": 1,
      "fieldTypeName": "CHAR",
      "fieldNullable": 1,
      "attrName": "ccyProdMktPrcCde",
      "jsonPath": "ccyProdMktPrcCde",
      "parent": "[ROOT]",
      "instrument": "ALL",
      "classification": "DMD",
      "mandatory": "O",
      "dataUsage": "Price",
      "logicalEntityName": null,
      "sampleData": "AUD = Australian dollar ^ CAD = Canadian dollar ^ EUR = Euro ^ GBP = Pound sterling ^ NZD = New Zealand dollar ^ JPY = Japanese yen ^ CHF = Swiss franc",
      "businessName": "Currency Product Market Price Code",
      "businessDefinition": "Denotes the currency code of the product price presented in the market. Usually it is in ISO format. (e.g. USD)",
      "graphQLType": "String",
      "validations": [
        {
          "given": "ccyProdMktPrcCde==null",
          "rules": {
            "spel": {
              "expression": "prodBidPrcAmt==null and prodOffrPrcAmt==null and prodMktPrcAmt==null and prodNavPrcAmt==null",
              "message": "If Market Price Currency[Currency Product Market Price Code] is empty, Bid Price[Product Bid Price Amount], Offer Price[Product Offer Price Amount], Closing Price Amount[Product Market Price Amount], NAV Price[Product Net Asset Value Price Amount] must be blank or enter the Market Price Currency[Currency Product Market Price Code]."
            }
          }
        },
        {
          "rules": {
            "referenceData": {
              "cdvTypeCde": "BASECCY"
            }
          }
        }
      ]
    },
    {
